  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jason-vasilev/profile-page/issues"
  },
  "homepage": "https://github.com/jason-vasilev/profile-page#readme",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "echo \"Error: no test specified\" && exit 1",
